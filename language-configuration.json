{
    "comments": {
        "lineComment": "//",
        //"blockComment": [ "#(", ")#" ]
    },
    "brackets": [
        [
            "(",
            ")"
        ],
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ]
    ],
    "autoClosingPairs": [
        //{ "open": "\\", "close": "\\" },
        //{ "open": "{", "close": "}" },
        //{ "open": "[", "close": "]" },
        //{ "open": "'", "close": "'", "notIn": ["string"] },
        //{ "open": "\"", "close": "\"", "notIn": ["string"] }
    ],
    "surroundingPairs": [
        //["\\", ":"],
        //["{", "}"],
        //["[", "]"],
        //["'", "'"],
        //["\"", "\""]
    ],
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#dd0000",
                "fontStyle": "italic"
            }
        }
    ],
    "wordPattern": "=(?= )|set_list_item|\\|([ \\w+ ])|=>|i64|index1|list|test",
}